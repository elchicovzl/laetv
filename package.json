  "scripts": {
    "build-clean": "rimraf dist/**/*",
    "build-server": "./node_modules/.bin/babel ./server --out-dir ./dist/server",
    "build-client": "NODE_ENV=production webpack --config ./build/webpack.prod.js -p --progress --profile --colors",
    "build-all": "npm run build-clean && npm run build-server & npm run build-client",
    "start": "npm run build-all && node ./dist/server/server.js",
    "start-dev": "./node_modules/.bin/babel-node ./server/server.js",
    "start-prod": "NODE_ENV=production PORT=3000 ./node_modules/.bin/babel-node ./dist/server/server.js",
    "test": "",
    "eslint": "eslint .",
    "jscs": "jscs ."
  },
  "dependencies": {
    "autoprefixer": "^6.0.3",
