    "json-loader": "^0.5.3",
    "mocha": "^2.3.3",
    "mocha-jsdom": "^1.0.0",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.6.0",
    "react": "^0.14.3",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-transform-hmr": "^1.0.0",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.1.2",
    "sinon": "^1.16.1",
    "sinon-chai": "^2.8.0",
