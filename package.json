    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.0.5",
    "bootstrap": "^4.0.0-alpha.2",
    "chai": "^3.2.0",
    "css-loader": "^0.19.0",
    "eslint": "^1.5.0",
    "eslint-plugin-react": "^3.4.2",
    "express": "^4.13.3",
    "express-handlebars": "^2.0.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.1",
